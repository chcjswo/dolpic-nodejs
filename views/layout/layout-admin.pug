doctype html
html
	head
		meta(charset='utf-8')
		meta(name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no")
		title= title
		include ../partials/stylesheets-admin
	body
		div#divLoadingBar(title='기다림의 미학.....' align='center' style='display:none;')
			img(src='/images/ajax-loader.gif')

		<!-- Static navbar -->
		nav.navbar.navbar-default.navbar-fixed-top
			.container
				.navbar-header
					button.navbar-toggle.collapsed(type='button' data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar")
						span.sr-only Toggle navigation
						span.icon-bar
						span.icon-bar
						span.icon-bar
					a.navbar-brand(href='/dolpic-admin/admin') 돌픽 어드민
				#navbar.navbar-collapse.collapse
					ul.nav.navbar-nav
						li(class=menu.menu1): a(href='/dolpic-admin/HashTagList') 해쉬태그 관리
						li(class=menu.menu2): a(href='/dolpic-admin/usersList') 회원 관리
						li(class=menu.menu3): a(href='/') 돌픽 사이트로
		div.container
			.page-header
				h1= subTitle
			block content

		input(type="hidden" name="infoMessage" value=messages())

		include ../partials/javascripts-admin